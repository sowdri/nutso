{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["ES2016", "DOM"],
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "target": "es6",
    "module": "nodenext",
    "outDir": "dist",
    "sourceMap": true
  },
  "exclude": ["node_modules", "dist"]
}
